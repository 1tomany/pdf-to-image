{
    "name": "1tomany/pdf-to-image",
    "description": "A simple library that converts the pages of a PDF to an image.",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "keywords": [
        "pdf",
        "poppler",
        "pdftoppm"
    ],
    "require": {
        "php": ">=8.4",
        "psr/container": "^2.0",
        "symfony/filesystem": "^7.2",
        "symfony/process": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\PdfToImage\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\PdfToImage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
